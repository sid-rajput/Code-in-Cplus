\\Program to check the size of data types and their default values\\
#include <iostream>

using namespace std;

int main()
{
  cout <<"size of char:"<< sizeof(char)<< "bytes" <<endl;
  cout <<"size of int:"<< sizeof(int)<< "byte" <<endl;
  cout <<"size of float:"<< sizeof(float)<< "byte" <<endl;
  cout <<"size of double:"<< sizeof(double)<< "byte" <<endl;
   cout << "Size of short: " << sizeof(short) << " byte" << endl;
    cout << "Size of unsigned: " << sizeof(unsigned) << " bytes" << endl;
    cout << "Size of signed " << sizeof(signed) << " bytes" << endl;
    cout << "Size of long: " << sizeof(long) << " bytes" << endl;
    char ch;
    float f;
    int i;
    double d;
    long l;
    signed s;
    short st;

    cout << ch << endl;
    cout << f << endl;
    cout << i << endl;
    cout << d << endl;
    cout << l << endl;
    cout << s << endl;
    cout << sh << endl;



    return 0;


}





\\Reverse of a number\\
#include<iostream>
using namespace std;
int main()
{
    cout<<"reverse of a number"<<endl;
    cout<<"Enter the num:";
    long int a ,  f, rev =0;
    cin>>a;

   while(a>0)
   {
      f=a%10;
      rev=rev*10+f;
      a = a/10;


   }
   cout<<endl;
   cout<<"rev number"<<rev;
   return 0;
}

